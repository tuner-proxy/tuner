{
  "extends": "@tsconfig/node22/tsconfig.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.tmp/tsconfig.tsbuildinfo",
    "composite": true,
    "rootDir": "./src",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true
  }
}
